{
  "name": "queuebone",
  "version": "0.4.1",
  "description": "Node.js job queue",
  "keywords": [
    "postgres",
    "pg",
    "job-queue"
  ],
  "license": "MIT",
  "contributors": [
    "Sebastian Riedel <kraih@mojolicious.org>",
    "Jan Kunzmann <jan-github@phobia.de>"
  ],
  "type": "module",
  "exports": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "lib/",
    "migrations/",
    "vendor/"
  ],
  "scripts": {
    "build": "npx tsc --build ./",
    "build:bench": "npm run build && npm run bench",
    "build:clean": "npm run clean && npm run build",
    "build:coverage": "npm run build && npm run coverage",
    "build:lint": "npm run build && npm run lint",
    "build:node": "npm run build && node",
    "build:test": "npm run build && npm test",
    "build:watch": "npm run build -- --watch",
    "clean": "rm -rf tsconfig.tsbuildinfo lib",
    "coverage": "c8 tap --disable-coverage --allow-empty-coverage test/*.js",
    "coverage:ci": "c8 --reporter lcovonly tap --disable-coverage --allow-empty-coverage test/*.js",
    "lint": "eslint \"src/**/*.ts\" \"src/*.ts\"",
    "lint:fix": "npm run lint -- --fix",
    "prepare": "husky install",
    "prepublishOnly": "npm run build",
    "publish:minor": "npm version minor && npm publish",
    "publish:patch": "npm version patch && npm publish",
    "test": "tap --disable-coverage --allow-empty-coverage"
  },
  "lint-staged": {
    "**/*.{js,ts,tsx}": [
      "eslint --fix"
    ],
    "**/*": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "js-yaml": "^4.1.0",
    "pg": "^8.7.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.3",
    "@eslint/js": "^9.8.0",
    "@types/busboy": "^1.5.0",
    "@types/eslint__js": "^8.42.3",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^20.0.0",
    "@types/pg": "^8.7.0",
    "@types/react": "^18.2.39",
    "@types/stack-utils": "^2.0.1",
    "@types/tap": "^15.0.3",
    "@types/tough-cookie": "^4.0.2",
    "@types/ws": "^8.5.3",
    "@vitest/coverage-istanbul": "^2.0.4",
    "c8": "^8.0.0",
    "eslint": "^9.11.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.9.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "sort-package-json": "^2.10.1",
    "tap": "^18.0.0",
    "typescript": "~5.5.4",
    "typescript-eslint": "^8.7.0"
  },
  "packageManager": "pnpm@9.0.2+sha512.8bedf31de42705b97ff89330bbe177954ad5703a92fa894be0fb4d45b5dfab59a21f65a8419ad1cae82dd16fac2d41a8c343775e80efe7dab6d80b0408db2fd6",
  "engines": {
    "node": ">= 18"
  }
}
